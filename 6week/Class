public class UMC {
	
	static class EvenOdd{
		public static boolean isEven(int num) {
			if(num % 2 == 0) {
				return true;
			}
			else {
				return false;
			}
		}
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int i=1; i<=10; i++) {
			if(EvenOdd.isEven(i)) {
				System.out.println(i + " is even.");
			}
		}
	}

}
