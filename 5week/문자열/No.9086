import java.util.*;

public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
	    int n = sc.nextInt();
	    
	    for(int i=0; i<n;i++){
	        String s = sc.next();
	        int len = s.length();
	        System.out.println(s.charAt(0) + ""+ s.charAt(len-1));
	    }
    }
}
