import java.util.Scanner;

public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int n = sc.nextInt();
		int times = n / 4;
		String ans = "";
		
		for(int i=0; i<times;i++){
		    ans += "long ";
		}
		
		System.out.println(ans + "int");
	}
}
